pipeline {
    agent any

    stages {
        stage('Pull source code from SCM') {
            steps {
                echo 'Hello World'
            }
        }
    
        stage('Build/Compile and Unit Test') {
            steps {
                echo 'Build'
            }
        }
        
        stage('Code Coverage') {
            steps {
                echo 'Code Coverage'
            }
        }
        
        stage('Quality Scan') {
            steps {
                echo 'Quality Scan'
            }
        }
        
        stage('Security Scan') {
            steps {
                echo 'Security Scan'
            }
        }
        
        stage('Push to S3') {
            steps {
                echo 'Push'
            }
        }
}
}

